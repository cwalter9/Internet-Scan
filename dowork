#!/bin/bash

stime=`date +%s`

cd /admin/iscan

. functions

penable=`cat config | grep ProcEnable | gawk -F= '{ print $2 }'`

if [ $penable -eq 0 ]; then
  dblog dowork "Exiting Processing disabled"
  exit 3
fi

bash randadd &
bash filladdr &
bash scanadd &
bash procscan &
bash hostwork &
bash proxycheck &
bash cleanaddrdir &
bash resethost &

ftime=`date +%s`
totime=`echo "$ftime - $stime" | bc`

funstop $processname
